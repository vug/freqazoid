{
  "name": "freqazoid",
  "version": "0.1.0",
  "description": "A real-time pitch detection software",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy:vendor": "node node_modules/copyfiles/copyfiles -f node_modules/material-design-lite/material.min.css node_modules/material-design-lite/material.min.js dist/vendor/",
    "build": "browserify src/main.js | babel --presets es2015 | uglifyjs --compress --mangle > dist/js/app.js",
    "watch": "watchify src/main.js -o dist/js/app.js",
    "deploy": "push-dir --dir=dist --branch=gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vug/freqazoid.git"
  },
  "author": {
    "name": "Ugur Guney",
    "email": "ugurguney@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vug/freqazoid/issues"
  },
  "homepage": "https://github.com/vug/freqazoid",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "browserify": "^13.1.0",
    "copyfiles": "^1.0.0",
    "push-dir": "^0.2.2",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "material-design-lite": "^1.2.0"
  }
}
